<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="user-scalable=no, initial-scale=1.0, maximum-scale=1.0">
    <title>@ViewBag.Title</title>
    <meta name="author" content="Frisson">
    <meta name="description" content="Frisson is a site designed to help fans connect with musicians, and for musicians to share their music.">
    <meta name="keywords" content="frisson, Frisson, audio, sharing, site, music">
    <!-- SOCIAL MEDIA META -->
    <meta property="og:description" content="Frisson is a site designed to help fans connect with musicians, and for musicians to share their music.">
    <meta property="og:image" content="http://frisson.audio/images/Frisson-logo.png">
    <meta property="og:site_name" content="Frisson">
    <meta property="og:title" content="Frisson">
    <meta property="og:type" content="website">
    <meta property="og:url" content="http://frisson.audio">
    <!-- TWITTER META -->
    <meta name="twitter:card" content="summary">
    <meta name="twitter:site" content="&#64;CHOLA">
    <meta name="twitter:creator" content="&#64;CHOLA">
    <meta name="twitter:title" content="CHOLA">
    <meta name="twitter:description" content="Frisson - Music Sharing Site">
    <meta name="twitter:image" content="http://frisson.audio/images/Frisson-logo.png">
    <!-- FAVICON FILES -->
    <link href="~/ico/apple-touch-icon-144-precomposed.png" rel="apple-touch-icon-precomposed" sizes="144x144">
    <link href="~/ico/apple-touch-icon-114-precomposed.png" rel="apple-touch-icon-precomposed" sizes="114x114">
    <link href="~/ico/apple-touch-icon-72-precomposed.png" rel="apple-touch-icon-precomposed" sizes="72x72">
    <link href="~/ico/apple-touch-icon-57-precomposed.png" rel="apple-touch-icon-precomposed">
    <link rel="icon" type="image/png" sizes="192x192" href="~/ico/android-icon-192x192.png">
    <link rel="icon" type="image/png" sizes="32x32" href="~/ico/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="96x96" href="~/ico/favicon-96x96.png">
    <link rel="icon" type="image/png" sizes="16x16" href="~/ico/favicon-16x16.png">
    <link rel="manifest" href="~/ico/manifest.json">
    <!-- CSS FILES -->
    @Styles.Render("~/Content/css")
    <link href="~/Content/chola/animate.css" rel="stylesheet">
    <link href="~/Content/chola/ionicons.css" rel="stylesheet">
    <link href='~/Content/chola/settings.css?rev=4.6.0&#038;ver=4.0' rel='stylesheet' type='text/css' media='all' />
    <link href="~/Content/chola/jquery.fancybox.css" rel="stylesheet">
    <link href="~/Content/chola/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/chola/base.css" rel="stylesheet">
    <link href="~/Content/chola/style.css" rel="stylesheet">
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
          <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
          <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    @RenderSection("styles", required: false)
</head>
<body>
    <script src="~/Scripts/chola/modernizr.custom.js"></script>
    <div class="loading">
        <img src="~/images/Frisson-logo-light.png" alt="Image"> <span>Loading</span>
        <div class="loader"></div>
    </div>
    <!-- end loading -->
    <div class="dark-overlay"></div>
    <!-- end dark-overlay -->
    <div class="collapse cbp-spmenu cbp-spmenu-vertical cbp-spmenu-left" id="bs-example-navbar-collapse-1">
        <img src="~/images/Frisson-logo-light.png" alt="Image">
        <ul class="nav navbar-nav">
            <li> <a href="@Url.Action("Index", "Home")" class="dark-transition">home</a> </li>
            <li> <a href="@Url.Action("About", "Home")" class="dark-transition">about us</a> </li>
            <li> <a href="@Url.Action("Contact", "Home")" class="dark-transition">contact</a> </li>
            @if (System.Web.HttpContext.Current.User.Identity.IsAuthenticated)
            {
                <li><a href="@Url.Action("Index", "Admin")" class="dark-transition">admin</a></li>
            }
        </ul>
        <!-- end nav -->
    </div>
    <!--end collapse -->
    <header>
        <div class="col-sm-6 no-padding">
            <button type="button" class="toggle-menu menu-left push-body menu-button" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1"> <span> MENU</span> </button>
            <img src="~/images/Frisson-logo-dark.png" alt="Image" class="logo">
        </div>
        <!-- end col-6 -->
        <div class="col-sm-6 no-padding text-right">
            <ul class="social-media">
                <li><a href="mailto:frisson@frisson.namespacemedia.com"><i class="ion-email"></i></a></li>
                <li><a href="https://www.facebook.com/frissonaudio"><i class="ion-social-facebook"></i></a></li>
                <li><a href="https://twitter.com/FrissonAudio"><i class="ion-social-twitter"></i></a></li>
                <li><a href="https://plus.google.com/117557104047859971446"><i class="ion-social-googleplus-outline"></i></a></li>
                @*<li><a href="#"><i class="ion-social-tumblr"></i></a></li>*@
            </ul>
        </div>
        <!-- end col-6 -->
    </header>
    <div class="left-border"></div>
    <div class="right-border"></div>
    <!-- end border left & right -->
    <div class="wrapper">
        @RenderBody()
    </div>
    <!-- end wrapper -->
    <footer class="position-fixed">
        <div class="col-xs-6 no-padding"> <span class="hidden-xs">© 2015 Copyright Chola - Digital Creative Agency</span> <span class="visible-xs">© 2015 Chola</span> </div>
        <div class="col-xs-6 no-padding text-right"><span>Created by themezinho</span></div>
    </footer>
    <!-- end footer -->
    <!-- JQUERY FILES -->
    <script src="~/Scripts/chola/jquery.min.js"></script>
    <script type="text/javascript">
        (function ($) {
            $(window).load(function () {
                $(".loading").addClass("fadeout");

                $("[name='EmailAddress']").val(null);
            });
        })(jQuery);

        $ = jQuery;
    </script>
    <script src="/Scripts/chola/bootstrap.min.js"></script>
    <script src="/Scripts/chola/jPushMenu.js"></script>
    <script src="/Scripts/chola/v2p.js"></script>
    <script src="/Scripts/chola/jquery.fancybox.js"></script>
    <script src="/Scripts/chola/classie.js"></script>
    <script src="/Scripts/chola/main.js"></script>
    <script src="/Scripts/chola/masonry.js"></script>
    <script src="/Scripts/chola/isotope.min.js"></script>
    <script src="/Scripts/chola/jquery.tubular.1.0.js"></script>
    <script src='/Scripts/chola/jquery.themepunch.tools.min.js' type='text/javascript'></script>
    <script src='/Scripts/chola/jquery.themepunch.revolution.js' type='text/javascript'></script>
    <script src='/Scripts/chola/settings.js' type='text/javascript'></script>
    <script src="/Scripts/chola/scripts.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>